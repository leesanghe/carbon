/*지마켓 산스*/
@font-face {
    font-family: 'GmarketSans';
    font-style: normal;
    font-weight: 200;
    src: local('GmarketSans'),
        url('/cbs/images/font/GmarketSansLight.woff2') format('woff2'), 
        url('/cbs/images/font/GmarketSansLight.woff') format('woff'),
        url('/cbs/images/font/GmarketSansLight.ttf') format('truetype'),
        url('/cbs/images/font/GmarketSansLight.otf') format('opentype'),
        url('/cbs/images/font/GmarketSansLight.eot') format('embedded-opentype');
}

@font-face {
    font-family: 'GmarketSans';
    font-style: normal;
    font-weight: 400;
    src: local('GmarketSans'),
        url('/cbs/images/font/GmarketSansMedium.woff2') format('woff2'), 
        url('/cbs/images/font/GmarketSansMedium.woff') format('woff'),
        url('/cbs/images/font/GmarketSansMedium.ttf') format('truetype'),
        url('/cbs/images/font/GmarketSansMedium.otf') format('opentype'),
        url('/cbs/images/font/GmarketSansMedium.eot') format('embedded-opentype');
}

@font-face {
    font-family: 'GmarketSans';
    font-style: normal;
    font-weight: 700;
    src: local('GmarketSans'),
        url('/cbs/images/font/GmarketSansBold.woff2') format('woff2'), 
        url('/cbs/images/font/GmarketSansBold.woff') format('woff'),
        url('/cbs/images/font/GmarketSansBold.ttf') format('truetype'),
        url('/cbs/images/font/GmarketSansBold.otf') format('opentype'),
        url('/cbs/images/font/GmarketSansBold.eot') format('embedded-opentype');
}