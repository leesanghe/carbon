    url(/cbs/images/font/NotoSansKR-Regular.woff) format('woff'),
    url(/cbs/images/font/NotoSansKR-Regular.otf) format('opentype');
}
@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 500;
    src: url(/cbs/images/font/NotoSansKR-Medium.woff2) format('woff2'),
    url(/cbs/images/font/NotoSansKR-Medium.woff) format('woff'),
    url(/cbs/images/font/NotoSansKR-Medium.otf) format('opentype');
}
@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 700;
    src: url(/cbs/images/font/NotoSansKR-Bold.woff2) format('woff2'),
    url(/cbs/images/font/NotoSansKR-Bold.woff) format('woff'),
    url(/cbs/images/font/NotoSansKR-Bold.otf) format('opentype');
}
@font-face {
    font-family: 'Noto Sans KR';
    font-style: normal;
    font-weight: 900;
    src: url(/cbs/images/font/NotoSansKR-Black.woff2) format('woff2'),
    url(/cbs/images/font/NotoSansKR-Black.woff) format('woff'),
