#qna {
  display: none;
}

.qBox {
  background-color: #e6e7ee;
  text-align: center;
  border-radius: 20px;
  font-size: 24px;
  width: 80%
}

/* styles.css 또는 원하는 파일명 */
.answerBox {
  text-align: center;
}

.btn-primary1 {
    background-color: #e6e7ee
}

.btn-primary1:hover, .btn-primary1:focus {
  background-color: #92bff0;
  color: white;
}

.status {
  height: 20px;
  width: 80%;
  background-color: white;
  border-radius: 20px;
}

.statusBar {
  height: 100%;
  border-radius: 20px;
  /* Permalink - use to edit and share this gradient: https://colorzilla.com/gradient-editor/#febbbb+0,fe9090+52,ff5c5c+100;Red+3D+%231 */
  background: #92bff0;
  /* Old browsers */
  background: -moz-linear-gradient(top, #92bff0 0%, #87e3ed 52%, #afcbed 100%);
  /* FF3.6-15 */
  background: -webkit-linear-gradient(top, #92bff0 0%, #87e3ed 52%, #afcbed 100%);
  /* Chrome10-25,Safari5.1-6 */
  background: linear-gradient(to bottom, #92bff0 0%, #87e3ed 52%, #afcbed 100%);
  /* W3C, IE10+, FF16+, Chrome26+, Opera12+, Safari7+ */
  filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#92bff0', endColorstr='#ff5c5c', GradientType=0);
  /* IE6-9 */
}
